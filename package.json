{
  "name": "lottery-bot",
  "version": "1.0.2",
  "description": "Randomly pick some team members as lottery winner!",
  "main": "index.js",
  "keywords": [
    "RingCentral",
    "bot",
    "poll"
  ],
  "scripts": {
    "ngrok": "ngrok http -region ap 6066",
    "start": "node -r @babel/register -r dotenv/config ./src/server/run-server.js",
    "deploy": "npm run build-lambda && npm run deploy-lambda",
    "fix": "standard --fix",
    "compile": "babel src/server --out-dir dist/server",
    "c": "webpack serve --config build/webpack.config.js",
    "build": "npm run clean && npm run compile",
    "view": "node build/view.js",
    "clean": "node build/clean.js",
    "build-lambda": "npm run clean && node build/build && npm run view",
    "deploy-lambda": "node build/deploy",
    "watch": "cd deploy && ../node_modules/.bin/sls logs -f app -t",
    "dynamo": "node -r @babel/register -r dotenv/config ./build/dynamo-local.js",
    "lint": "standard"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "@ringcentral/sdk": "^4.6.1",
    "dayjs": "^1.10.7",
    "express": "^4.17.1",
    "handlebars": "^4.7.7",
    "json-deep-copy": "^1.1.6",
    "morgan": "^1.10.0",
    "multipart-mixed-parser": "^0.1.3",
    "nanoid": "^3.1.25",
    "pug": "^3.0.0",
    "ringcentral-chatbot-core": "^1.2.1",
    "serverless-http": "^2.7.0"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/plugin-proposal-class-properties": "^7.16.5",
    "@babel/plugin-proposal-decorators": "^7.16.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@babel/register": "^7.16.5",
    "@electerm/antd-dayjs-webpack-plugin": "^1.0.7",
    "@electerm/pug-html-loader": "^1.1.6",
    "antd": "^4.17.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-lodash": "^3.3.4",
    "classnames": "^2.3.1",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.3.1",
    "dotenv": "^10.0.0",
    "dynamodb-local": "0.0.31",
    "file-loader": "^6.2.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "mini-css-extract-plugin": "^2.4.5",
    "ngrok": "^4.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "serverless": "^2.69.1",
    "serverless-deployment-bucket": "^1.5.1",
    "shelljs": "^0.8.4",
    "sqlite3": "^5.0.2",
    "standard": "^16.0.4",
    "style-loader": "^3.3.1",
    "stylus": "^0.56.0",
    "stylus-loader": "^6.2.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ringcentral/lottery-bot.git"
  },
  "author": "Drake Zhao <drake.zhao@ringcentral.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ringcentral/lottery-bot/issues"
  },
  "homepage": "https://github.com/ringcentral/lottery-bot#readme",
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "*-compiled.js",
      "dist",
      "external"
    ]
  }
}
